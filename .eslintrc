{
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/forbid-prop-types": 0,
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "minProperties": 4,
          "consistent": true
        },
        "ObjectPattern": {
          "multiline": false
        }
      }
    ],
    "consistent-return": 0,
    "jsx-a11y/href-no-hash": 1,
    "jsx-a11y/no-marquee": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-absolute-path": 0,
    "import/first": 0,
    "object-shorthand": [
      "error",
      "always"
    ],
    "prefer-arrow-callback": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "func-names": [
      "error",
      "never"
    ],
    "camelcase": 0,
    "import/extensions": [
      "off",
      "never"
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
